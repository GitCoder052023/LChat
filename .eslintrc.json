{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "rules": {
      "no-console": "off",
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "indent": ["error", 2]
    },
    "ignorePatterns": ["Public/style.css"]
  }